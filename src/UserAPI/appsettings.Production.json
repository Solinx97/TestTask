{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Database": {
    // property 'DefaultConnection' stored in User Secrets as it secret (have username/password for DB Server), provided by VS. Please, add 'Database:DefaultConnection' in User Secrets (as it stored localy on PC) or add property 'DefaultConnection' here to connect to DB
  },
  "JWT": {
    // property 'Key' stored in User Secrets as it secret (key to validate token), provided by VS. Please, add 'JWT:Key' in User Secrets (as it stored localy on PC) or add property 'Key' here
    "Issuer": "user-api"
  }
}
